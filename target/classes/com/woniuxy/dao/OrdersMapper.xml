	<select id="findByTid" resultMap="BaseResultMap">
		select * from
		orders o where o.fsid in(
		select fs.fsid from show_venue sv join
		film_session fs
		on
		sv.svid = fs.svid
		where tid = #{tid}
		)
	</select>


	<select id="findByStartTime" resultMap="BaseResultMap">
